{
    "manifest_version": 3,
    "name": "Quick Pins",
    "version": "1.0.0",
    "description": "Минималистичное всплывающее окно для хранения избранных ссылок с favicon.",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "action": {
        "default_title": "Мои быстрые ссылки",
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    },
    "permissions": ["storage"],
    "host_permissions": []
}
